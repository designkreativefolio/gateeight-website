</style>

<script>
  (function () {
    const SELECTOR = '.kpi-counter';
    const counters = document.querySelectorAll(SELECTOR);
    if (!counters.length) return;

    const fmtCompact = new Intl.NumberFormat('en', { notation: 'compact', maximumFractionDigits: 0 });

    function formatValue(el, value) {
      const suffix = el.dataset.suffix || '';
      const plus = el.dataset.plus === 'true';
      const useCompact = el.dataset.format === 'compact';
      if (useCompact) {
        const compact = fmtCompact.format(value);
        return compact + (plus ? '+' : '');
      }
      return Math.floor(value).toString() + suffix;
    }

    function animate(el) {
      const target = Number(el.dataset.target || 0);
      const duration = Number(el.dataset.duration || 1500);
      const startTime = performance.now();

      function tick(now) {
        const progress = Math.min((now - startTime) / duration, 1);
        const eased = 1 - Math.pow(1 - progress, 3); // easeOutCubic
        const current = target * eased;
        el.textContent = formatValue(el, current);
        if (progress < 1) requestAnimationFrame(tick);
        else el.textContent = formatValue(el, target);
      }
      requestAnimationFrame(tick);
    }

    const seen = new WeakSet();
    const io = 'IntersectionObserver' in window
      ? new IntersectionObserver(entries => {
          entries.forEach(entry => {
            if (!entry.isIntersecting) return;
            entry.target.querySelectorAll(SELECTOR).forEach(c => {
              if (!seen.has(c)) { seen.add(c); animate(c); }
            });
          });
        }, { threshold: 0.3 })
      : null;

    const section = document.querySelector('.metrics-section');
    if (io && section) io.observe(section);
    else counters.forEach(animate); // fallback
  })();
</script>

